{
    "manifest_version": 2,
   
    "name": "RandomBookMark",
    "description": "Choose daily 5 bookmarks for study!",
    "version": "1.0",
   
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "bookmarks",
      "storage"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
  }